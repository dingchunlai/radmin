  <script type="text/javascript">
    function s_s(){
      var model = document.getElementById("model").value;
      var pay = document.getElementById("pay").value;
      var style = document.getElementById("style").value;
      var color = document.getElementById("color").value;
      var use = document.getElementById("use").value;
      var way = document.getElementById("way").value;
      var case_name = document.getElementById("case_name").value;
      n =  encodeURI(case_name)
      var url = "/zhuangxiu/tuku/search_case?m="+model+"&"+"p="+pay+"&"+"s="+style+"&"+"c="+color+"&"+"u="+use+"&"+"w="+way+"&"+"n="+n
      window.open(url,"_self")
    }
    function onKeypress(e){
      var isie = (document.all) ? true : false;
      var key;
      if(isie){
        key = window.event.keyCode;
      }else{
        key = e.which;
      }
      if(key==13){
        s_s()
      }
    }
  </script>
