  <script type="text/javascript">
    function page(page){
      var patrn=/^[0-9]{1,200}$/;
      var total_pages = document.getElementById("p_count").value;
      var tagf = document.getElementById("tagf").value;
      var tags = document.getElementById("tags").value;
      var url = "";
      if(tagf == "jinghua" || tagf == "nippon"){
        url = "/zhuangxiu/"+tagf+"/"+page;
      } else {
        url = "/zhuangxiu/"+tagf+"-"+tags+"-"+page;
      }
      if(page != "" && parseInt(page) > 0 && parseInt(page) <= total_pages && patrn.test(page)){
        window.open(url,"_self")
      }
    }
    function onKeypress(e){
      var isie = (document.all) ? true : false;
      var key;
      if(isie){
        key = window.event.keyCode;
      }else{
        key = e.which;
      }
      if(key==13){
        var page1 = document.getElementById("p_num").value;
        var patrn=/^[0-9]{1,200}$/;
        var total_pages = document.getElementById("p_count").value;
        if(page1 != "" && parseInt(page1) > 0 && parseInt(page1) <= total_pages && patrn.test(page1)){
          page(page1);
        }
      }
    }
  </script>
  <script language="javascript" type="text/javascript">
