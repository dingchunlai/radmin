        <style type="text/css">
          #datalist {
            border-collapse:collapse;
            border:1px solid #BED0E2;
          }
          #datalist th
          {
            text-align:center;
            font-family:Verdana;
            padding: 5px 15px 3px 15px;
            letter-spacing: 12px;
            background-color: #45719C;
            font-size: 14px;
            color: white;
            font-weight: normal;
            text-decoration: none;
            border:1px solid white;
          }
          #datalist td {
            color: #757575;
            font-family:Verdana;
            padding: 5px 8px 5px 8px;
            letter-spacing: 1px;
            font-size: 9pt;
            border:1px solid #D9E3EE;
          }
          #datalist a {
            color: #2853FF;
            text-decoration: none;
          }
          #datalist a:hover {
            color: #FD352B;
            text-decoration: none;
          }
        </style>
