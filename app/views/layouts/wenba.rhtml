<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <title>用户管理 - <%=WEBSITE_NAME%>[<%=WEBSITE_DOMAIN%>]</title>
    <meta name="keywords" content="<%=WEBSITE_NAME%>" />
    <meta name="description" content="<%=WEBSITE_NAME%>" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="robots" content="all" />
    <meta name="author" content="MSN: qq1713173@hotmail.com" />
    <script type="text/javascript" src="/javascripts/public2.js" charset="gb2312"></script>
    <script type="text/javascript" src="/javascripts/WebCalendar.js"></script>
    <link href="/stylesheets/wrapper01.css" rel="stylesheet" type="text/css" />
    <style type="text/css">
      #datalist {
        border-collapse:collapse;
        border:1px solid #BED0E2;
      }
      #datalist th
      {
        text-align:center;
        font-family:Verdana;
        padding: 5px 15px 3px 15px;
        letter-spacing: 12px;
        background-color: #45719C;
        font-size: 14px;
        color: white;
        font-weight: normal;
        text-decoration: none;
        border:1px solid white;
      }
      #datalist td {
        color: #757575;
        font-family:Verdana;
        padding: 5px 8px 5px 8px;
        letter-spacing: 1px;
        font-size: 9pt;
        border:1px solid #D9E3EE;
      }
      #datalist a {
        color: #2853FF;
        text-decoration: none;
      }
      #datalist a:hover {
        color: #FD352B;
        text-decoration: none;
      }
      .nav{
        background:url(http://www.51hejia.com/images/zhuangxiu805/iconav.png) left center no-repeat;
        line-height:27px;
        color:#666666;
        padding-left:15px;
        margin:10px 12px 0 12px;
      }
    </style>
  </head>
  <body>
    <div id="H_wrapper">
      <%=render :file => "/bgmenu.rhtml"%>
    </div>

    <div id="L_wrapper">

      <input onclick="self.location.href='/wenba/examine_topics';" style="width:83px" type="button" value="问题审核列表" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/time_mechanisms';" style="width:83px" type="button" value="时间设置" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/topic_list';" style="width:83px" type="button" value="问题列表" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/section_topics';" style="width:83px" type="button" value="区间问题管理" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/focus_topic';" style="width:83px" type="button" value="焦点问题" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/notice_list';" style="width:83px" type="button" value="公告栏" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/wenba_word_filter';" style="width:83px" type="button" value="关键词过滤" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/tag_manage';" style="width:83px" type="button" value="分类管理" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/manage_users';" style="width:83px" type="button" value="用户管理" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/wenba_users';" style="width:83px" type="button" value="月用户发贴排行" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/users';" style="width:83px" type="button" value="单个用户管理" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/topics';" style="width:83px" type="button" value="删除贴管理" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/operations';" style="width:83px" type="button" value="操作记录" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/examine_posts';" style="width:83px" type="button" value="问题回复审核列表" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
      <input onclick="self.location.href='/wenba/posts';" style="width:83px" type="button" value="问题回复列表" class="btn1_mouseout" onmouseover="this.className='btn1_mouseover'" onmouseout="this.className='btn1_mouseout'" /><br />
    </div>

    <div id="R_wrapper">
      <div class="nav"><strong></strong><%= location -%></div>
      <%=yield%>
    </div>

    <iframe width="0" height="0" name="hideiframe" id="hideiframe"></iframe>
  </body>
</html>