/* CSS Document */

*{padding:0;}
body{
	font-family:"宋体"; font-size:12px; line-height:20px;
	color:#000;
	margin:0 auto;

}
body{
	text-align:center;
	}
.Div_center{
	margin:0 auto;
	width:960px;
	background-color:#fff;
}
div,tr{
	text-align:left;
	}
p{
	line-height:170%;
	color:#000;
	margin:0 auto;
	padding:0px;
	}
ul,li{
	padding:0px;
	margin:0px;
	list-style-type: none;
}
img{border:0px}
html,body { height:100%;} 
a{text-decoration:none; text-align:left}
img{border:0}
li{list-style:none}
td {text-align:left}
.clear{clear:both}





.Top_title {width:960px; margin:0 auto; position:relative; border-bottom:1px solid #BFBFBF; border-top:1px solid #BFBFBF; overflow:hidden; margin-top:8px;}
.Top_title ul{ height:47px; float:left; margin-left:10px;}
.Top_Navtion li{display:inline; font-size:14px; line-height:47px;}
.Top_Navtion li a{ color:#7F7F7F; background:url(http://www.51hejia.com/images/Images1014/GrayLine.gif) no-repeat 0 40%; padding-left:10px; padding-right:10px; padding-top:5px; padding-bottom:5px;}
.Top_Navtion .Linew a{ color:#7F7F7F; background:none;}
.Top_Navtion .RedNavZhong a{ background:none; background-color:#F20000; color:#fff; padding-left:10px; padding-right:10px; padding-top:5px; padding-bottom:5px;}
.Top_title img{ float:left;}
.Top_title span img{ margin-top:11px;}
.Top_Anniu{ right:7px; float:right; line-height:47px;}
.Top_Anniu img{ margin-right:5px; margin-top:13px;}
.Top_Anniu span {float:left;}
.Top_Navtion li a:hover{ background:none; background-color:#F20000; color:#fff; padding-left:10px; padding-right:10px; padding-top:5px; padding-bottom:5px; }
.Pindao{ height:40px; margin-top:5px;}
.Pindao li { display:inline; height:40px; cursor:pointer; float:left;}
.Pindao li a{ color:#575757;}
.Pindao li a:hover{ color:#ff6600;}
.Pindao .line{ background-color:#ECECEC; width:2px; height:40px; margin-left:10px; margin-right:10px;}
.Pindao .Red{ color:#D8403F;}



